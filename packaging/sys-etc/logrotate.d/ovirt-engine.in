"@ENGINE_LOG@/engine.log" "@ENGINE_LOG@/server.log" "@ENGINE_LOG@/ui.log" {
	daily
	minsize 10M
	missingok
	copytruncate
	rotate 20
	compress
}

"@ENGINE_LOG@/host-deploy"/*.log {
	monthly
	missingok
	compress
	nocreate
	rotate 1
}

"@ENGINE_LOG@/ansible"/*.log {
	monthly
	missingok
	compress
	nocreate
	rotate 1
}

"@ENGINE_LOG@/dump"/*.hprof {
	daily
	missingok
	compress
	nocreate
	rotate 1
}
