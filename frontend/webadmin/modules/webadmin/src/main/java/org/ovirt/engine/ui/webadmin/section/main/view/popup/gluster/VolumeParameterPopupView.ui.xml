<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:org.ovirt.engine.ui.common.widget.dialog"
	xmlns:e="urn:import:org.ovirt.engine.ui.common.widget.editor">

	<ui:style>
		.description textarea{
			height: 130px;
		}
	</ui:style>
	
	<d:SimpleDialogPanel width="450px" height="300px">
		<d:content>
			<g:VerticalPanel>
				<e:ListModelListBoxEditor ui:field="keyListEditor" />
				<e:EntityModelTextAreaLabelEditor  addStyleNames="{style.description}" ui:field="descriptionEditor"/>
				<e:EntityModelTextBoxEditor ui:field="valueEditor" />
			</g:VerticalPanel>
		</d:content>
	</d:SimpleDialogPanel>
	
</ui:UiBinder>