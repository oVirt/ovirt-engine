<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
    xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:d="urn:import:org.ovirt.engine.ui.common.widget.dialog">

    <ui:style>
        .iframe {
            position: absolute;
            width: 100%;
            height: 100%;
            border-style: none;
        }
    </ui:style>

    <d:SimpleDialogPanel ui:field="dialogPanel">
        <d:content>
            <g:HTMLPanel>
                <iframe ui:field="iframeElement" class="{style.iframe}" frameBorder="0" />
            </g:HTMLPanel>
        </d:content>
    </d:SimpleDialogPanel>

</ui:UiBinder>
