<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module>
      <!-- Inherit the core Web Toolkit stuff.                        -->
      <inherits name='com.google.gwt.user.User'/>

      <!--  inherit SmartGWT stuff. -->
      <inherits name="com.smartgwt.SmartGwtNoTheme" />

      <inherits name="org.ovirt.engine.ui.userportal.UserPortalSkin" />

      <!--  inherit css based theme -->
      <inherits name='com.google.gwt.user.theme.standard.Standard'/>

      <!-- inherit engine module -->
	  <inherits name='org.ovirt.engine.ui.GwtExtension'/>
      <inherits name='org.ovirt.engine.ui.UICommon'/>

      <inherits name='com.google.gwt.rpc.RPC'/>
      <inherits name='com.google.gwt.regexp.RegExp' />

      <!-- Logging -->
	  <inherits name="com.google.gwt.logging.Logging"/>
	  <set-property name="gwt.logging.logLevel" value="FINEST"/>
	  <!-- Should be FALSE in production build -->
	  <set-property name="gwt.logging.enabled" value="FALSE"/>
	  <set-property name="gwt.logging.simpleRemoteHandler" value="ENABLED" />
	  <set-property name="gwt.logging.popupHandler" value="DISABLED" />

      <!-- Specify the app entry point class.                         -->
      <entry-point class='org.ovirt.engine.ui.userportal.client.UserPortal'/>

      <!-- Specify the application specific style sheet.              -->
      <stylesheet src='UserPortal.css' />
</module>
