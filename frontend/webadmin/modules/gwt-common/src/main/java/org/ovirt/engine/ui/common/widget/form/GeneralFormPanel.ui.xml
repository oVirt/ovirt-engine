<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
		.contentPanel {
			width: 100%;
		}

		@external formPanel_viewerSeparator;
		.formPanel_viewerSeparator {
			float: left;
			width: 20px;
			height: 100%;
		}

		@external formPanel_viewer;
		.formPanel_viewer {
			float: left;
			padding-right: 16px;
			padding-left: 16px;
			line-height: 18px;
		}

		@external formPanel_itemName;
		.formPanel_itemName {
			white-space: nowrap;
			font-family: Arial,sans-serif;
			color: black;
		}

		@external formPanel_itemValue;
		.formPanel_itemValue {
			white-space: nowrap;
			font-family: Arial,sans-serif;
			color: #777777;
			padding-left: 4px;
			padding-top: 4px;
			padding-bottom: 4px;
		}

		@external formPanel_viewerNamesColumn;
		.formPanel_viewerNamesColumn {
		}

		@external formPanel_viewerValuesColumn;
		.formPanel_viewerValuesColumn {
			min-width: 100px;
			width: 100%;
		}
	</ui:style>

	<g:HorizontalPanel ui:field="contentPanel" addStyleNames="{style.contentPanel}"/>

</ui:UiBinder>
