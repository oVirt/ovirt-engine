<FindBugsFilter>
     <Match>
       <!--
        findbugs complains that we are having empty block of catch exception:
        catch (Exception e) {}
        This is a test code that close ssh socket and have nothing to do if the
        connection is already closed.
       -->
       <Class name="org.ovirt.engine.core.utils.ssh.TransferTest" />
       <Method name="tearDown"/>
       <Bug code="DE"/>
     </Match>

     <!--
      findbugs complains that we don't use innerText in XmlNode
      We do use that code for reading/writing xml from legacy code

      findbugs reason:
      UrF: Unread public/protected field (URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD)
     -->
     <Match>
       <Class name="org.ovirt.engine.core.utils.ovf.xml.XmlNode" />
       <Field name="innerText"/>
       <Bug code="UrF"/>
     </Match>

     <!--
      findbugs complains that we don't use attributes in XmlNode
      We do use that code for reading/writing xml from legacy code

      findbugs reason:
      UrF: Unread public/protected field (URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD)
     -->
     <Match>
       <Class name="org.ovirt.engine.core.utils.ovf.xml.XmlNode" />
       <Field name="attributes"/>
       <Bug code="UrF"/>
     </Match>

     <!--
      findbugs complains that HTTP parameter is directly written to
      HTTP header output. This servlet is used from frontend and url
      is already encode, so the warning is not relevant.

      findbugs reason:
      HRS: HTTP parameter directly written to HTTP header output (HRS_REQUEST_PARAMETER_TO_HTTP_HEADER)
     -->
     <Match>
       <Class name="org.ovirt.engine.core.utils.servlet.RedirectServlet" />
       <Bug code="HRS"/>
     </Match>

     <!--
     findbugs complains that the OsRepository.init method should not
     be public, but it needs to be because it is the mechanism that
     we use to lazily initialzy the OS repository.

     findbugs reason:
     ME: Public enum method unconditionally sets its field (ME_ENUM_FIELD_SETTER)
     -->
     <Match>
       <Class name="org.ovirt.engine.core.utils.OsRepositoryImpl" />
       <Bug code="ME"/>
     </Match>

     <!--
      ignore all warning on these classes as they are auto generated by javacc
      -->
     <Match>
       <Class name="org.ovirt.engine.core.utils.osinfo.OsInfoParser" />
     </Match>
     <Match>
       <Class name="org.ovirt.engine.core.utils.osinfo.OsInfoParserConstants" />
     </Match>
    <Match>
       <Class name="org.ovirt.engine.core.utils.osinfo.OsInfoParserTokenManager" />
     </Match>
     <Match>
       <Class name="org.ovirt.engine.core.utils.osinfo.ParseException" />
     </Match>
     <Match>
       <Class name="org.ovirt.engine.core.utils.osinfo.SimpleCharStream" />
     </Match>
     <Match>
       <Class name="org.ovirt.engine.core.utils.osinfo.Token" />
     </Match>
     <Match>
       <Class name="org.ovirt.engine.core.utils.osinfo.TokenMgrError" />
     </Match>

</FindBugsFilter>
