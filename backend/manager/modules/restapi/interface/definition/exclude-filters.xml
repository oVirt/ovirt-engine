<FindBugsFilter>
    <!-- Ignore auto-generated code -->
    <Match>
        <Or>
            <Class name="~.*\.ObjectFactory" />
            <Class name="~^.*\.[^.]+ResourceHelper$" />
            <Package name="org.ovirt.engine.api.xml" />
            <Package name="org.ovirt.engine.api.json" />
        </Or>
    </Match>

    <!-- Exclude the object factories generated by the XJC compiler as
         they are very large, and findbugs doesn't like them: -->
    <Match>
        <Class name="~.*\.ObjectFactory" />
    </Match>
</FindBugsFilter>
